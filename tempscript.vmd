
        package require hbonds
        mol load psf ../../velocity_buehler/menton_set/polyisoleucine/processed/wowater.psf dcd ../../velocity_buehler/menton_set/polyisoleucine/processed/wowaterpulling.dcd
        set g1 [atomselect top "chain C"]
        set g2 [atomselect top "protein and not chain C"]

        set mol [molinfo top]
        set nf [molinfo $mol get numframes]
    
        for {set i 0} {$i < $nf} {incr i} {
        $g1 frame $i
        $g2 frame $i
        hbonds -sel1 $g1 -sel2 $g2 -frames $i:$i -dist 3.5 -ang 30 -writefile yes -upsel yes -plot no -type unique -detailout ../../velocity_buehler/menton_set/polyisoleucine/processed/hbonds_all/$i.hbdata
        }
        exit
        